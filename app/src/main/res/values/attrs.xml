<resources>
    <declare-styleable name="CustomCard">
        <attr name="cardName" format="string" />
        <attr name="cardDescription" format="string" />
        <attr name="personName" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomNotif">
        <attr name="request_or_proposal" format="string" />
        <attr name="description" format="string" />
        <attr name="person_name" format="string" />
        <attr name="date" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomEvent">
        <attr name="title" format="string" />
        <attr name="dateAndTime" format="string" />
        <attr name="location" format="string" />
        <attr name="state" format="string" />
        <attr name="userMoney" format="string" />
    </declare-styleable>
</resources>